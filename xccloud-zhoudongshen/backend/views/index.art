    <script src="../../web-pc/index/js/jquery.scrollTo.min.js"></script>
    <script src="../../web-pc/index/js/common-scripts.js"></script>




    <script>

            $f('plugin', 'user', 0);
            $f("df","logout",function(_data){
                $f("ajax.get","/backend/logout",_data,function(_data){window.location.replace("/login");},0)();
                
            });
    </script>
 

