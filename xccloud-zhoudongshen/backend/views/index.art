<body style="height:100%;">
<!--登陆弹窗-->
<div id="login" data=""></div>
<section id="container" >

    <!--header start-->
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="#" class="logo"><b>周东申艺术馆</b></a>
        <!--logo end-->

        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li><a id="loginName" style="background-color: transparent;border:0px !important" class="logout">{{userName[0].user_name}}</a></li>
                <li><a id="logoutbtn" class="logout" onclick="$f('logout',0)">退出</a></li>
            </ul>
        </div>

    </header>
    <!--header end-->
       
    <!--菜单-->
    <aside>
        <div id="sidebar"  class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">

                <li class="mt" onclick="$f('plugin', 'user', 0)">
                    <a class="active" >
                        <i class="fa fa-dashboard"></i>
                        <span>用户管理</span>
                    </a>
                </li>


                <li class="sub-menu" onclick="$f('plugin', 'wxuser', 0)">
                    <a href="javascript:;" >
                        <i class="fa fa-desktop"></i>
                        <span>微信用户管理</span>
                    </a>
                </li>
                <li class="sub-menu" onclick="$f('plugin', 'paintings', 0)">
                    <a href="javascript:;" >
                        <i class="fa fa-desktop"></i>
                        <span>画作管理</span>
                    </a>
                </li>
                 <li class="sub-menu">
                     <a href="javascript:;" >
                         <i class="fa fa-cogs"></i>
                         <span>文章管理</span>
                     </a>
                     <ul class="sub">
                         <li onclick="$f('plugin', 'news', 'news')"><a  >新闻管理</a></li>
                         <li onclick="$f('plugin', 'artist', 'artist')"><a>艺术家管理</a></li>

                     </ul>
                 </li>


                <li class="sub-menu">
                    <a href="javascript:;" >
                        <i class="fa fa-cogs"></i>
                        <span>活动管理</span>
                    </a>
                    <ul class="sub">
                        <li onclick="$f('plugin', 'onActivity', 'onActivity')"><a  >线上活动</a></li>
                        <li><a  >线下活动</a></li>

                    </ul>
                </li>

                <li class="sub-menu" onclick="$f('plugin', 'message', 0)" >
                    <a href="javascript:;" >
                        <i class="fa fa-desktop"></i>
                        <span>留言管理</span>
                    </a>
                </li>

            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>



